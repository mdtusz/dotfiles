[user]
  name = MikloÃÅs Tusz
  email = mdtusz@gmail.com
[github]
  user = mdtusz
[push]
  default = simple
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[advice]
  statusHints = true
[core]
  editor = vim
  excludesfile = ~/.gitignore
[alias]
  co = checkout
  d = diff --color-words
  psu = push --set-upstream
  reabse = rebase
  hist = log --graph --decorate --oneline
  tree = log --all --graph --decorate --oneline
[pager]
  branch = false
[status]
  submoduleSummary = true
