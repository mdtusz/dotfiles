# Shell aliases

alias pms='python2.7 -m SimpleHTTPServer'

alias ..='cd ..'
alias ...='cd .. && cd ..'
alias ....='cd .. && cd .. & cd ..'

alias red='redshift -O 1000'
alias blue='redshift -x'
alias invert='xcalib -invert -alter'

alias open='xdg-open'

alias dc='docker-compose'
alias tf='terraform'

alias ssh='TERM=xterm ssh'

alias bn='ddcutil setvcp 10'

alias ls='ls --color=tty --hyperlink=auto'
